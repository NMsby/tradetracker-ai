# TradeTracker AI - Environment Variables
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# AI Services
VITE_CLAUDE_API_KEY=your_claude_api_key_here
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_GOOGLE_VISION_API_KEY=your_google_vision_api_key_here

# Application Configuration
VITE_APP_NAME="TradeTracker AI"
VITE_APP_VERSION="1.0.0"

# Development Settings
VITE_DEBUG_MODE=true
VITE_API_BASE_URL=http://localhost:3000

# Feature Flags
VITE_ENABLE_VOICE_INPUT=true
VITE_ENABLE_RECEIPT_SCANNING=true
VITE_ENABLE_WHATSAPP_ALERTS=false
VITE_ENABLE_AI_PROCESSING=true
